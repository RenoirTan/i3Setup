*.font: xft:Noto Sans:size=12:antialias=true

Xft.dpi: 120
Xft.antialias: 1
Xft.autohint: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.lcdfilter: lcdlight
Xft.rgba: rgb

URxvt.font: xft:JetBrainsMono Nerd Font Mono:size=12:antialias=true
URxvt.boldFont: xft:JetBrainsMono Nerd Font Mono:bold:size=12:antialias=true
URxvt*termName: rxvt-256color
URxvt.depth: 32
URxvt*lineSpace: 1
URxvt.letterSpace: -1
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.scrollBar_right: true
URxvt*loginShell: true
URxvt.internalBorder: 5
URxvt.perl-ext: default,matcher
URxvt.perl-ext-common: selection-to-clipboard
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
